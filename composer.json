{
    "name": "majframe/routing",
    "description": "Majframe package for routing.",
    "type": "library",
    "minimum-stability": "stable",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Ond≈ôej Maxa",
            "homepage": "http://ondrej.maxa.expert",
            "email": "ondrej@maxa.expert"
        }
    ],
    "autoload": {
        "psr-4": {
            "Majframe\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Majframe\\": "tests/unit/"
        }
    },
    "require": {
        "php": "^7.4",
        "doctrine/annotations": "1.10.2",
        "haydenpierce/class-finder": "^0.4.2",
        "majframe/utils": "v1.0.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "codeception/codeception": "^4.1",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-asserts": "^1.0.0"
    },
    "scripts": {
        "codecept": "codecept run",
        "codecept-steps": "@codecept --steps"
    },
    "config": {
        "platform": {
            "php": "7.4"
        },
        "sort-packages": true
    }
}
